#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n,sum=0;
    do{
        scanf("%d",&n);
        if(n!=-1)
         sum=sum+n;
        
    }
    while(n!=-1);
    printf("%d",sum);
    return 0;
}
