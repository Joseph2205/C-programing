#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d %d",&a,&b);
    printf("The First value before Swapping:%d\n",a);
    printf("The Second value before Swapping:%d\n",b);
    c = b;
     b = a;
    printf("The First value after Swapping:%d\n",c);
        printf("The Second value after Swapping:%d\n",b);
 
    
         

    return 0;
}
