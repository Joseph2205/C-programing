#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define PI 3.14159265359


int main() {
    int side,length,width,radius,base1,side2,base2;
    scanf("%d %d %d %d %d %d %d",&side,&length,&width,&radius,&base1,&side2,&base2);
    printf("Perimeter of Square:%d\n",4*side);
    printf("Perimeter of Rectangle:%d\n",2*(length+width));
    printf("Perimeter of Circle:%.2f\n",floor(2*PI*radius*100)/100.0);
    printf("Perimeter of Triangle:%d\n",base1+base2+side2);
    return 0;
}
