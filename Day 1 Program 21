#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    int gain = (a*b)-(a*c);
    int profit = gain - 100;
    printf("Number of copies sold:%d\n",a);
    printf("Cost of each copy:%d\n",b);
    printf("Cost spent by agency on each newspaper:%d\n",c);
    printf("The profit obtained is Rs.%.2f",(float)profit);
    

     
    return 0;
}
