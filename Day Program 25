#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char arr[100];
    scanf("%s",arr);
    int a,b,c,d,e;
    scanf("%d\n%d\n%d\n%d\n%d",&a,&b,&c,&d,&e);
    int total =(a+b+c+d+e);
    
    float average = total/5.0;
    printf("Name of the Student:%s\n",arr);
    printf("Total marks:%d\n",total);
    
    printf("Average marks:%.1f",average);
           
    return 0;
}
