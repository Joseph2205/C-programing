#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a;
    scanf("%d",&a);
    if(a>0){
        printf("The Given Number %d is Positive.\n",a);
    }
    else if (a<0){
         printf("The Given Number %d is Negative.\n",a);
        
    }
    else if(a==0){
        printf("The Given Number %d is Zero.\n",a);
        
        
    }
    return 0;
}
