#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int even=0,odd=0,n;
    scanf("%d",&n);
    int arr[n];
    for(int i = 0;i<n;i++){
        scanf("%d",&arr[i]);
    }
     for(int i = 0;i<n;i++){
         if(arr[i]%2==0)
            even++;
             else if (arr[i]%2!=0)
                odd++;

     }
    if(even==n)
        printf("Even");
    else if(odd==n)
        printf("Odd");
    else 
        printf("Mixed");
    return 0;
}
